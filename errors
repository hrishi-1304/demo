#include<stdio.h>
#include<signal.h>
void sig_handler(int sig_num)
{
        printf("I am in signal handler for signal 1");

 

}
void main()
{
        int pid=getpid();
        printf("pid of the process= %d\n",pid);
        signal(1,sig_handler);
        while(1);
}
